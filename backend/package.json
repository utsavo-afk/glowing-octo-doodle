{
	"name": "some-api",
	"version": "1.0.0",
	"description": "Server supports websockets, prismaORM, express",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env nodemon",
		"prod": "cross-env NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
		"build": "cross-env tsc -p tsconfig.json",
		"deploy": "npm run build && npm run prod",
		"lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix ./.gitignore",
		"lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
		"lint": "npm run lint:format && npm run lint:fix",
		"demo": "concurrently \"npm run dev\" \"cd ../frontend/ && npm run dev\"",
		"dbpush": "npx prisma db push",
		"dbpull": "npx prisma db pull",
		"migrations:dev": "npx prisma migrate dev",
		"migrations:prod": "npx prisma migrate deploy"
	},
	"keywords": [],
	"author": "utsavo-afk",
	"license": "MIT",
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/dotenv-safe": "^8.1.2",
		"@types/express": "^4.17.13",
		"@types/morgan": "^1.9.3",
		"@types/node": "^16.11.7",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"concurrently": "^7.2.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-prettier": "^4.0.0",
		"nodemon": "^2.0.15",
		"pino-pretty": "^7.6.1",
		"prettier": "^2.4.1",
		"ts-node": "^10.4.0",
		"tsconfig-paths": "^3.11.0",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@prisma/client": "^3.14.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv-safe": "^8.2.0",
		"express": "^4.17.2",
		"helmet": "^5.0.2",
		"http-status-codes": "^2.2.0",
		"morgan": "^1.10.0",
		"pino": "^7.11.0",
		"prisma": "^3.14.0",
		"socket.io": "^4.5.0",
		"yup": "^0.32.11"
	}
}
